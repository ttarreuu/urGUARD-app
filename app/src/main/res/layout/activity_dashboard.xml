<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashboardActivity"
    android:padding="50dp"
    android:background="@color/black">

  <ImageView
      android:id="@+id/logo2_1"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:src="@drawable/long_logo"
      app:layout_constraintHeight_percent="0.05"
      app:layout_constraintLeft_toLeftOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      android:scaleType="fitStart"
      app:layout_constraintRight_toLeftOf="@id/btn_setting"
      app:layout_constraintRight_toRightOf="parent"/>


  <ImageButton
      android:id="@+id/btn_setting"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintDimensionRatio="1:1"
      android:src="@drawable/setting_icon"
      app:layout_constraintHeight_percent="0.04"
      app:layout_constraintRight_toRightOf="parent"
      app:layout_constraintTop_toTopOf="@id/logo2_1"
      app:layout_constraintBottom_toBottomOf="@id/logo2_1"
      android:scaleType="fitEnd"
      android:background="?android:selectableItemBackground"/>

  <FrameLayout
      android:id="@+id/frame2_1"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintDimensionRatio="1:1"
      app:layout_constraintWidth_percent="0.5"
      app:layout_constraintRight_toRightOf="parent"
      app:layout_constraintBottom_toBottomOf="parent"/>

  <FrameLayout
      android:id="@+id/frame2_2"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintDimensionRatio="1:1"
      app:layout_constraintWidth_percent="0.5"
      app:layout_constraintRight_toRightOf="@id/frame2_1"
      app:layout_constraintRight_toLeftOf="@id/frame2_1"
      app:layout_constraintBottom_toTopOf="@id/frame2_1"/>

  <FrameLayout
      android:id="@+id/frame2_3"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintDimensionRatio="1:1"
      app:layout_constraintWidth_percent="0.5"
      app:layout_constraintRight_toRightOf="parent"
      app:layout_constraintBottom_toTopOf="@id/frame2_2"/>

  <ImageButton
      android:id="@+id/btn_message"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintTop_toTopOf="@id/frame2_3"
      app:layout_constraintBottom_toBottomOf="@id/frame2_3"
      app:layout_constraintLeft_toLeftOf="@id/frame2_3"
      app:layout_constraintRight_toRightOf="@id/frame2_3"
      android:src="@drawable/message_icon"
      android:background="?android:selectableItemBackground"
      android:scaleType="centerInside"/>

  <ImageButton
      android:id="@+id/btn_sos"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintTop_toTopOf="@id/frame2_2"
      app:layout_constraintBottom_toBottomOf="@id/frame2_2"
      app:layout_constraintLeft_toLeftOf="@id/frame2_2"
      app:layout_constraintRight_toRightOf="@id/frame2_2"
      android:src="@drawable/sos_icon"
      android:background="?android:selectableItemBackground"
      android:scaleType="centerInside"/>

  <ImageButton
      android:id="@+id/btn_location"
      android:layout_width="0dp"
      android:layout_height="0dp"
      app:layout_constraintTop_toTopOf="@id/frame2_1"
      app:layout_constraintBottom_toBottomOf="@id/frame2_1"
      app:layout_constraintLeft_toLeftOf="@id/frame2_1"
      app:layout_constraintRight_toRightOf="@id/frame2_1"
      android:src="@drawable/location_icon"
      android:background="?android:selectableItemBackground"
      android:scaleType="centerInside"/>


</androidx.constraintlayout.widget.ConstraintLayout>